<h1 class="doc-header">All Active Doctors</h1>
<div class="container all-doctors-page">
	<div class="doctors-table">
		<table class="table table-condensed table-hover table-bordered">
			<thead>
				<tr>
					<th>Name</th>
					<th>Title</th>
					<th>Main Specialty</th>
					<th>Total Rating</th>
				</tr>
			</thead>
			{{#each doctors}}
			<tbody>
				<tr>
					<td>{{this.first_name}} {{this.last_name}}</td>
					<td>{{this.title}}</td>
					<td>{{this.primary_specialty}}</td>
					<td>{{this.total}}</td>
				</tr>
			</tbody>
			{{/each}}
		</table>
	</div>
</div>